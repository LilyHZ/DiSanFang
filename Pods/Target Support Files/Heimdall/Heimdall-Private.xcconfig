#include "Heimdall.xcconfig"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/Heimdall" "${PODS_ROOT}/Headers/Public"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES
SWIFT_INCLUDE_PATHS[sdk=iphoneos*] = ${HEIMDALL_SWIFT_INCLUDE_PATHS}
SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*] = ${HEIMDALL_SWIFT_INCLUDE_PATHS}